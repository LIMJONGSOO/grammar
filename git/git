git init
git config --list
git clone [url]
git remote -v  - 저장소 확인
git remote add  [저장소 이름]  [url]
git remote remove  [저장소 이름]
git remote set-url [저장소 이름] [URL.git]
git status
git add .
git add [파일이름]
git commit -m "message"
git push
git pull
git branch
git branch [new branch name]
git checkout [branch]
git switch [branch]
git merge [병합할 branch]
git branch -d [branch 명]
git tag <tag name>  -tag 추가
git log --decorate 
git tag -am "내용" <tag name>
git tag -n
git tag -d  <tag name>
git add sample.txt
git commit --amend
git revert HEAD